{
	"type": "object",
	"additionalProperties": false,
	"required": [
		"id",
		"name"
	],
	"properties": {
		"id": {
			"type": "string"
		},
		"name": {
			"$ref": "#/definitions/name"
		},
		"names": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/name"
			},
			"default": []
		},
		"dob": {
			"$ref": "#/definitions/dob"
		},
		"addresses": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/address"
			}
		},
		"phones": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/phone"
			}
		},
		"emails": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/email"
			}
		},
		"relatives": {
			"type": "array",
			"items": {
				"$ref": "#"
			}
		}
	},
	"definitions": {
		"name": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"first": {
					"type": ["string", "null"],
					"default": null
				},
				"middle": {
					"type": ["string", "null"],
					"default": null
				},
				"last": {
					"type": ["string", "null"],
					"default": null
				},
				"prefix": {
					"type": ["string", "null"],
					"default": null
				},
				"suffix": {
					"type": ["string", "null"],
					"default": null
				},
				"display": {
					"type": "string",
					"default": ""
				},
				"_score": {
					"type": "number",
					"default": 0
				}
			}
		},
		"dob": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"day": {
					"anyOf": [
						{
							"type": "number",
							"min": 0,
							"max": 31
						},
						{
							"type": "null"
						}
					],
					"default": null
				},
				"month": {
					"anyOf": [
						{
							"type": "number",
							"min": 1,
							"max": 12
						},
						{
							"type": "null"
						}
					],
					"default": null
				},
				"year": {
					"anyOf": [
						{
							"type": "number",
							"min": 1000,
							"max": 9999
						},
						{
							"type": "null"
						}
					],
					"default": null
				},
				"display": {
					"type": "string",
					"default": ""
				},
				"_score": {
					"type": "number",
					"default": 0
				}
			}
		},
		"address": {
			"type": "object",
			"required": [],
			"additionalProperties": false,
			"properties": {
				"street": {
					"type": ["string", "null"],
					"default": null
				},
				"street2": {
					"type": ["string", "null"],
					"default": null
				},
				"city": {
					"type": ["string", "null"],
					"default": null
				},
				"state": {
					"anyOf": [
						{
							"type": "string",
							"format": "/[A-Z]{2}/"
						},
						{
							"type": "null"
						}
					],
					"default": null
				},
				"zip": {
					"anyOf": [
						{
							"type": "string",
							"format": "/[0-9]{5}/"
						},
						{
							"type": "null"
						}
					],
					"default": null
				},
				"zip4": {
					"anyOf": [
						{
							"type": "string",
							"format": "/[0-9]{4}/"
						},
						{
							"type": "null"
						}
					],
					"default": null
				},
				"county": {
					"type": ["string", "null"],
					"default": null
				},
				"country": {
					"oneOf": [
						{
							"type": "string",
							"format": "/[A-Z]{2}/"
						},
						{
							"type": "null"
						}
					],
					"default": null
				},
				"latitude": {
					"type": ["number", "null"],
					"default": null
				},
				"longitude": {
					"type": ["number", "null"],
					"default": null
				},
				"display": {
					"type": "string",
					"default": ""
				},
				"_score": {
					"type": "number",
					"default": 0
				}
			}
		},
		"phone": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"number": {
					"type": ["string", "null"],
					"default": null
				},
				"display": {
					"type": "string",
					"default": ""
				},
				"_score": {
					"type": "number",
					"default": 0
				}
			}
		},
		"email": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"address": {
					"type": ["string", "null"],
					"default": null
				},
				"display": {
					"type": "string",
					"default": ""
				},
				"_score": {
					"type": "number",
					"default": 0
				}
			}
		}
	}
}